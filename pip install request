   "source": [
    "all_info ={\n",
    "    \"Items name\":[],\n",
    "    \"New Price\":[],\n",
    "    \"Old Price\":[],\n",
    "    \"Discounted %\":[],\n",
    "    \"Number of people rated\":[],\n",
    "    \"Rated Score\":[]\n",
    "}\n",
    "containers=soup.findAll(\"div\",{\"class\":\"info\"})\n",
    "for container in containers:\n",
    "    try:\n",
    "        container.find(\"h3\",{\"class\":\"name\"}).text\n",
    "        all_info[\"Items name\"].append(item_name)\n",
    "    except:\n",
    "        all_info[\"Items name\"].append(np.nan) \n",
    "    try:\n",
    "        item_new_price=container.find(\"div\",{\"class\":\"prc\"}).get_text()\n",
    "        all_info[\"New Price\"].append(item_new_price)\n",
    "    except:\n",
    "            all_info[\"New Price\"].append(np.nan)\n",
    "    try:\n",
    "        item_old_price=container.find(\"div\",{\"class\":\"old\"})\n",
    "        all_info[\"Old Price\"].append(item_old_price)\n",
    "    except:\n",
    "            all_info[\"Old Price\"].append(np.nan)\n",
    "    try:\n",
    "        discounted_cent= container.find(\"div\",{\"class\":\"bdg _dsct _sm\"}).text\n",
    "        all_info[\"Discounted %\"].append(discounted_cent)\n",
    "    except:\n",
    "            all_info[\"Discounted %\"].append(np.nan)\n",
    "    try:\n",
    "        no_of_pple_rated= container.find(\"div\",{\"class\":\"rev\"}).text.split(\"(\")[1].strip(\")\")\n",
    "        all_info[\"Number of people rated\"].append(no_of_pple_rated)\n",
    "    except:\n",
    "            all_info[\"Number of people rated\"].append(np.nan)\n",
    "    try:\n",
    "        rated_score= container.find(\"div\",{\"class\":\"rev\"}).text.split()[0]\n",
    "        all_info[\"Rated Score\"].append(rated_score)\n",
    "    except:\n",
    "            all_info[\"Rated Score\"].append(np.nan)\n",
    "        \n",
    "df=pd.DataFrame(all_info)  \n",
    "df\n",
    "        \n",
    "        "
   ]
